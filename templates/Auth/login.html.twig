{% extends 'base.html.twig' %}{% block title %}    Login{% endblock %}{% block body %}        <section class="w3l-contact-info-main" id="contact">            <div class="contact-sec	editContent">                <div class="container">                    <div class="main-titles-head text-center">                        <h3 class="header-name editContent">                            Login                        </h3>                        <hr>                        {% for message in app.flashes('notice') %}                            <div class="alert alert-dismissible alert-warning" id="alert">                                <button type="button" class="close" data-dismiss="alert">&times;</button>                                <h4 class="alert-heading">Warning!</h4>                                <p class="mb-0">{{ message }}</p>                            </div>                        {% endfor %}                    </div>                    <div class="map-content-9 mt-5">                        {{ form_start(form) }}                        <div class="twice">                            {{ form_widget(form.email,{ 'attr': {'class': 'input-sm form-control email','placeholder':'Email'} }) }}                        </div>                        <div class="twice">                            {{ form_widget(form.password,{ 'attr': {'class': 'input-sm form-control','placeholder':'Password'} }) }}                        </div>                        <div class="text-right">                            {{ form_widget(form.Login,{ 'attr': {'class': 'btn btn-danger btn-sm','id':'submit','value':'login'} }) }}                        </div>                        {{ form_end(form) }}                    </div>                </div>            </div>        </section>{% endblock %}