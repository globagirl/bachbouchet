{% extends 'base.html.twig' %}{% block title %}    Signup{% endblock %}{% block body %}    <section class="w3l-contact-info-main" id="contact">        <div class="contact-sec	editContent">            <div class="container">                <div class="main-titles-head text-center">                    <h3 class="header-name editContent">                        Join Bachbouchet                    </h3>                    <hr>                </div>                <div class="map-content-9 mt-5">                    {{ form_start(form) }}                    <div class="twice">                        {{ form_widget(form.email,{ 'attr': {'class': 'input-sm form-control email','placeholder':'Email'} }) }}                    </div>                    <div class="twice">                        {{ form_widget(form.password,{ 'attr': {'class': 'input-sm form-control','placeholder':'Password'} }) }}                    </div>                    <div class="text-right">                        {{ form_widget(form.SignUp,{ 'attr': {'class': 'btn btn-danger btn-sm','id':'submit','value':'Sign up'} }) }}                    </div>                    {{ form_end(form) }}                </div>            </div>        </div>    </section>{% endblock %}